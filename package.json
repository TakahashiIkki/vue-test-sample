{
  "name": "vue-test",
  "version": "0.0.1",
  "scripts": {
    "test": "npm run deleteCoverage && npm run karma",
    "karma": "$(npm bin)/karma start karma.conf.js --single-run",
    "deleteCoverage": "rm -rf coverage/",
    "clean": "rm -rf node_modules/ && npm cache clean && npm run deleteCoverage"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "mocha": "^3.4.2",
    "karma": "^1.6.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "phantomjs-prebuilt": "^2.1.14"
  },
  "dependencies": {
    "vue": "^2.3.3"
  }
}
